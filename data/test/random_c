#include <stdlib.h>
#include <stdio.h>
#include <time.h>

int main(int argc,char* argv[])
{
	if (argc != 3)
	{	
		printf("Usage : %s nb_min(int) nb_max(int)\n", argv[0]);
		exit(1);
	}
	srand(time(NULL));
	int a=atoi(argv[1]);
	int b=atoi(argv[2]);
	int r = (rand() % (b-a+1)) + a;
	printf("%d\n", r);
	return 0;
} 

